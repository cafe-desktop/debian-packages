Subject: [PATCH] ctk,Makefile: move 'ctkcolorsel' and 'ctkcolorseldialog' from
 'ctk_private_h_sources' to 'ctk_public_h_sources'

---
 ctk/Makefile.am | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/ctk/Makefile.am b/ctk/Makefile.am
index 9aab6c274..938d5a723 100644
--- a/ctk/Makefile.am
+++ b/ctk/Makefile.am
@@ -176,6 +176,8 @@ ctk_public_h_sources = 		\
 	ctkcolorchooser.h	\
 	ctkcolorchooserwidget.h	\
 	ctkcolorchooserdialog.h	\
+	ctkcolorsel.h		\
+	ctkcolorseldialog.h	\
 	ctkcolorutils.h		\
 	ctkcombobox.h		\
 	ctkcomboboxtext.h	\
@@ -419,8 +421,6 @@ ctk_private_h_sources =		\
 	ctkcolorpickershellprivate.h	\
 	ctkcolorplaneprivate.h	\
 	ctkcolorscaleprivate.h	\
-	ctkcolorsel.h		\
-	ctkcolorseldialog.h		\
 	ctkcolorchooserprivate.h	\
 	ctkcomboboxprivate.h	\
 	ctkcomposetable.h	\
