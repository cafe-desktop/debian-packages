#!/bin/sh

set -e

case "$1" in
  upgrade)
    if dpkg --compare-versions "$2" le "0.28-2"; then
      if [ -f '/etc/X11/Xsession.d/52libkanberra-ctk-module_add-to-ctk-modules' ]; then
        rm -f '/etc/X11/Xsession.d/52libkanberra-ctk-module_add-to-ctk-modules'
      fi
    fi
esac

#DEBHELPER#
exit 0
